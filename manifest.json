{
  "manifest_version": 3,
  "name": "Google Form Filler",
  "version": "1.0",
  "description": "Automatically fills Google Forms with predefined information",
  "permissions": ["activeTab", "scripting"],
  "host_permissions": ["*://docs.google.com/forms/*"],
  "content_scripts": [
    {
      "matches": ["*://docs.google.com/forms/*"],
      "js": ["js/content.js"]
    }
  ],
  "icons": {
    "48": "icons/icon.png",
    "128": "icons/icon.png"
  }
}